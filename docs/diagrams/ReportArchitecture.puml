@startuml
!include style.puml

rectangle "main.js\n(window.app)" as main
rectangle "v_zoom.js" as v_zoom
rectangle "v_ramp.js" as v_ramp
rectangle "v_summary.js" as v_summary
rectangle "v_authorship.js" as v_authorship
rectangle "v_segment.js" as v_segment
database "api.js" as api

main -down-> v_summary
v_summary -down-> v_zoom
v_summary -> v_ramp
v_zoom -> v_ramp

main -> v_authorship
v_authorship -> v_segment

main <- api : summary.json
v_summary <- api : projectName/commits.json
v_authorship <- api : projectName/authorship.json
@enduml
