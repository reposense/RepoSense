{
    "extends": [
        "../.eslintrc.json"
    ],
    "globals": {
        "assert": true,
        "Cypress": true,
        "cy": true,
        "describe": true,
        "expect": true,
        "it": true,
        "beforeEach": true
    },
    "rules": {
        "no-unused-expressions": "off",
        "semi": ["error", "always"],
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "space-before-function-paren": ["error", "never"],
        "comma-dangle": ["error", "never"]
    }
}
