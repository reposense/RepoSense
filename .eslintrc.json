{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-param-reassign": "warn",
        "arrow-parens": ["error", "always"],
        "indent": ["error", 2, {
            "MemberExpression": 2,
            "FunctionDeclaration": { "body": 1, "parameters": 2 }
        }]
    }
}
