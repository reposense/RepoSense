{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-param-reassign": "warn",
        "arrow-parens": ["error", "always"],
        "indent": ["error", 2, {
            "MemberExpression": 2,
            "CallExpression": { "arguments": 2 },
            "ArrayExpression": 2,
            "ignoredNodes": [
                "ConditionalExpression"
            ]
        }]
    }
}
